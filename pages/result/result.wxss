.result-title{
  font: 1.2em caption;
  color: #2d8cf0;
  text-align: center;
  margin: 10rpx;
  padding-bottom: 10rpx;
  border-bottom: 1px solid #e9eaec;
  letter-spacing:5px;
}

.evaluations-section{
  margin: 10rpx;
}
.evaluation-area{
  margin: 10rpx;
}
.evaluation-area .group-name {
  margin: 10rpx;
  color: #2d8cf0;
}
.evaluation-area .evaluation-content{
  padding: 10rpx;
  padding-left: 26rpx;
}
.evaluation-header .evaluation-score {
  float: right;
}
.evaluation-level, .evaluation-age, .evaluation-score {
  font-style: italic;
  color: #ff9900;
}

.evaluation-summary{
  margin: 30rpx;
  text-align: center;
}